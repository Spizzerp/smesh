{
  "name": "stealth-privacy-bundles",
  "version": "1.0.0",
  "description": "Privacy SDK bundles for MeshStealth app",
  "private": true,
  "type": "module",
  "scripts": {
    "bundle:privacycash": "node bundle-privacycash.js",
    "bundle:shadowpay": "node bundle-shadowpay.js",
    "bundle:all": "npm run bundle:privacycash && npm run bundle:shadowpay",
    "clean": "rm -f ../Sources/StealthCore/Privacy/Resources/*.js",
    "prebuild": "npm run clean",
    "build": "npm run bundle:all"
  },
  "dependencies": {
    "@shadowpay/client": "^0.1.1",
    "@shadowpay/core": "^0.1.1",
    "@shadowpay/server": "^0.1.1",
    "privacycash": "1.1.10"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "esbuild": "^0.24.0",
    "localforage": "^1.10.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
